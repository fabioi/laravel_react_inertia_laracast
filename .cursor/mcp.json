{
    "mcpServers": {
        "laravel-boost": {
            "command": "php",
            "args": [
                "artisan",
                "boost:mcp"
            ]
        },
        "playwright": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-playwright"
            ],
            "env": {
                "PLAYWRIGHT_BASE_URL": "http://127.0.0.1:8001"
            }
        }
    }
}